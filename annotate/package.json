{
    "name":"annotate",
    "displayName": "annotate",
    "description": "insert annotate",
    "version": "0.0.1",
    "publisher": "my extension",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],

    "activationEvents": [
         "onCommand:extension.NoteFile",
         "onCommand:extension.extension.NoteFuntion",
         "onCommand:extension.NoteCow"
        // "*"
    ],
    
    "main": "./extension",


    "contributes": {
        "keybindings": [
            {
                "command": "extension.NoteFile",
                "key": "ctrl+alt+h",
                "when":"editorTextFocus"
            },
            {
                "command": "extension.NoteFuntion",
                "key": "ctrl+alt+f",
                "when":"editorTextFocus"
            },
            {
                "command": "extension.NoteCow",
                "key": "ctrl+alt+l",
                "when":"editorTextFocus"    
            }
        ]
    },


    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },


    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}